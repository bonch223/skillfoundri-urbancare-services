{
  "name": "urbancare-backend",
  "version": "1.0.0",
  "description": "UrbanCare Marketplace Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server-simple.ts",
    "dev:production": "nodemon src/server-production.ts",
    "build": "tsc -p tsconfig.production.json",
    "start": "node dist/server-minimal.js",
    "start:simple": "node dist/server-simple.js",
    "start:mongodb": "node server-mongodb.js",
    "migrate": "npm run build && node dist/database/migrate.js",
    "migrate:dev": "ts-node src/database/migrate.ts",
    "seed": "npm run build && node dist/database/seed.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "railway:deploy": "npm run build && npm run migrate && npm start"
  },
  "keywords": [
    "urbancare",
    "marketplace",
    "api",
    "typescript",
    "express",
    "postgresql"
  ],
  "author": "UrbanCare Team",
  "license": "MIT",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
